{
    "manifest_version": 2,
    "name" : "Search & Destroy: SAT Vocabulary",
    "description" : "Destroys the SAT",
    "version" : "1.0",
    "icons" : {"16": "icon.png",
                "80": "icon2.png"},
    "background" : {
        "scripts" : ["background.js"]
    },
    "permissions" : ["activeTab", "tabs", "*://*/*"
    ],


    "content_scripts" : [{
        "matches" : ["*://*/*"],
        "css": ["foundation.css", "normalize.css"],
        "js" : ["jquery-1.10.2.min.js","foundation.js", "foundation.tooltips.js", "wordlist.js","HTMLSynonymReplaceFunction.js"],
        "run_at": "document_end",
        "web_accessible_resources": ["HTMLSynonymReplaceFunction.js"]
    }]
}
