{
    "manifest_version": 2,
    "name" : "A simple Found Text Demo",
    "description" : "Bla",
    "version" : "1.0",
    "background" : {
        "scripts" : ["background.js"]
    },
    "permissions" : ["activeTab", "tabs", "*://*/*"
    ],
    //"page_action" : {
    //    "default_icon" : "icon.png"
    //},

    "content_scripts" : [{
        "matches" : ["*://*/*"],
        "css": ["foundation.css", "foundation.min.css", "normalize.css"],
        "js" : ["jquery-1.10.2.min.js","foundation.js", "foundation.tooltips.js", "wordlist.js","HTMLSynonymReplaceFunction.js"],
        "run_at": "document_end",
        "web_accessible_resources": ["HTMLSynonymReplaceFunction.js"]
    }]
}
